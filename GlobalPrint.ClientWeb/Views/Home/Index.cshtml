@{
    ViewBag.Title = "GlobalPrint Home";
    Layout = "~/Views/Shared/_NavigationLayout.cshtml";
}

@section Scripts{
    <script src="https://maps.google.com/maps/api/js" type="text/javascript"></script>
    <script src="~/Resources/Scripts/home.js"></script>
    <script src="~/Resources/Scripts/home/index.js"></script>
}
@section Stylesheets {
    <link href="~/Resources/Stylesheets/home/index.css" rel="stylesheet" />
}
<div class="home_index container-full">

    <div id="sidebar-wrapper" class="hidden fixed-top col-xs-12 col-sm-5 col-md-4 col-lg-3">
        <form class="form-horizontal" role="form">
            <input type="hidden" id="printerInfoPrinterID" />
            <div>
                <div class="form-group row">
                    <label for="printerInfoIsAvailable" class="control-label vertical-center text-left">Доступность</label>
                    <input type="checkbox" class="printer-info-is-available form-control" disabled="disabled" id="printerInfoIsAvailable" />
                    <label class="control-label vertical-center" for="printerInfoIsAvailable"></label>
                </div>
                <div class="form-group">
                    <label class="control-label text-left">Оператор печати</label>
                    <div>
                        <textarea rows="10" class="form-control" readonly="readonly" id="printerInfoOperator"></textarea>
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label text-left">Услуги и цены</label>
                    <div>
                        <textarea rows="10" class="form-control" readonly="readonly" id="printerInfoPrices"></textarea>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div>
                    <button class="btn btn-default" type="button" id="printerInfoPrint">Печать</button>
                    <button class="btn btn-default" type="button" id="printerInfoClose">Закрыть</button>
                </div>
            </div>
        </form>
    </div>
    <div id="googlemaps" class="googlemaps">
    </div>

</div>